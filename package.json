{
  "name": "budget-tracker-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint -c ./.eslintrc.cjs --ignore-path ./.eslintignore .",
    "dev": "vite",
    "prod": "vite --mode production",
    "docker-build": "docker build . -t letehaha/budget-tracker-fe",
    "docker-push": "docker push letehaha/budget-tracker-fe",
    "docker-compose": "npm run docker-build && npm run docker-push",
    "start": "npm run dev",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest",
    "storybook": "storybook dev -p 6006",
    "test:e2e": "cypress run",
    "test:e2e:interact": "cypress open --browser chrome --e2e",
    "git-pull": "git pull --recurse-submodules",
    "postinstall": "chmod +x ./post-install.sh && ./post-install.sh"
  },
  "type": "module",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@heroicons/vue": "^2.1.1",
    "@tanstack/vue-query": "^5.17.8",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "core-js": "^3.35.0",
    "date-fns": "^3.1.0",
    "highcharts": "^11.2.0",
    "highcharts-vue": "^1.4.3",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.1",
    "pinia": "^2.1.7",
    "register-service-worker": "^1.7.2",
    "vue": "3.4.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@storybook/addon-actions": "^7.6.7",
    "@storybook/addon-docs": "^7.6.7",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addons": "^7.6.7",
    "@storybook/cli": "^7.6.7",
    "@storybook/vue3": "^7.6.7",
    "@storybook/vue3-vite": "^7.6.7",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/compiler-sfc": "3.4.4",
    "@vue/eslint-config-airbnb": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "css-loader": "^6.8.1",
    "cypress": "^13.6.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.7",
    "storybook": "^7.6.7",
    "storybook-dark-mode": "^3.0.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.1.3",
    "vue-style-loader": "^4.1.3",
    "vue-tsc": "^1.8.27"
  },
  "volta": {
    "node": "20.10.0"
  }
}
