{
  "name": "budget-tracker-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint -c ./.eslintrc.js --ignore-path ./.eslintignore .",
    "dev": "vite",
    "prod": "vite --mode production",
    "docker-build": "docker build . -t letehaha/budget-tracker-fe",
    "docker-push": "docker push letehaha/budget-tracker-fe",
    "docker-compose": "npm run docker-build && npm run docker-push",
    "start": "npm run dev",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest",
    "storybook": "storybook dev -p 6006",
    "test:e2e": "cypress run",
    "test:e2e:interact": "cypress open --browser chrome --e2e",
    "git-pull": "git pull --recurse-submodules",
    "postinstall": "chmod +x ./post-install.sh && ./post-install.sh"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "core-js": "^3.32.0",
    "date-fns": "^2.30.0",
    "highcharts": "^11.1.0",
    "highcharts-vue": "^1.4.3",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.1",
    "pinia": "^2.1.6",
    "register-service-worker": "^1.7.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.2.3",
    "@storybook/addon-docs": "^7.2.3",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/addons": "^7.2.3",
    "@storybook/cli": "^7.2.3",
    "@storybook/vue3": "^7.2.3",
    "@storybook/vue3-vite": "^7.2.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "css-loader": "^6.8.1",
    "cypress": "^12.17.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.65.1",
    "storybook": "^7.2.3",
    "storybook-dark-mode": "^3.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.34.1",
    "vue-style-loader": "^4.1.3",
    "vue-tsc": "^1.8.8"
  }
}
