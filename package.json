{
  "name": "budget-tracker-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint -c ./.eslintrc.js --ignore-path ./.eslintignore",
    "dev": "vite",
    "prod": "vite --mode production",
    "docker-build": "docker build . -t letehaha/budget-tracker-fe",
    "docker-push": "docker push letehaha/budget-tracker-fe",
    "docker-compose": "npm run docker-build && npm run docker-push",
    "start": "npm run dev",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --passWithNoTests --config ./jest.config.js",
    "storybook": "start-storybook -p 6006",
    "test:e2e": "cypress run",
    "test:e2e:interact": "cypress open --browser chrome --e2e",
    "postinstall": "chmod +x ./post-install.sh && ./post-install.sh"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@vuelidate/core": "^2.0.0-alpha.42",
    "@vuelidate/validators": "^2.0.0-alpha.30",
    "core-js": "^3.23.3",
    "date-fns": "^2.28.0",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.0",
    "pinia": "^2.0.14",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/vue3": "^6.5.9",
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/vue3-jest": "^28.0.1",
    "css-loader": "^6.7.1",
    "cypress": "^10.3.0",
    "dotenv": "^16.0.1",
    "eslint": "^7.32.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.1.1",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^1.1.0",
    "markdown-loader-jest": "^0.1.1",
    "sass": "^1.55.0",
    "storybook-dark-mode": "^1.1.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4",
    "vite": "^3.1.3",
    "vite-svg-loader": "^3.6.0",
    "vue-style-loader": "^4.1.3",
    "vue-tsc": "^0.40.13"
  }
}
