{
  "name": "budget-tracker-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint -c ./.eslintrc.cjs --ignore-path ./.eslintignore .",
    "dev": "vite",
    "prod": "vite --mode production",
    "docker-build": "docker build . -t letehaha/budget-tracker-fe",
    "docker-push": "docker push letehaha/budget-tracker-fe",
    "docker-compose": "npm run docker-build && npm run docker-push",
    "start": "npm run dev",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest",
    "storybook": "storybook dev -p 6006",
    "test:e2e": "cypress run",
    "test:e2e:interact": "cypress open --browser chrome --e2e",
    "git-pull": "git pull --recurse-submodules",
    "postinstall": "chmod +x ./post-install.sh && ./post-install.sh"
  },
  "type": "module",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@tanstack/vue-query": "^5.17.19",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "core-js": "^3.35.1",
    "date-fns": "^3.3.1",
    "highcharts": "^11.3.0",
    "highcharts-vue": "^1.4.3",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.1",
    "lucide-vue-next": "^0.314.0",
    "pinia": "^2.1.7",
    "radix-vue": "^1.3.2",
    "register-service-worker": "^1.7.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "v-calendar": "^3.1.2",
    "vue": "3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@pinia/testing": "^0.1.3",
    "@storybook/addon-actions": "^7.6.10",
    "@storybook/addon-docs": "^7.6.10",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addons": "^7.6.10",
    "@storybook/cli": "^7.6.10",
    "@storybook/vue3": "^7.6.10",
    "@storybook/vue3-vite": "^7.6.10",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/compiler-sfc": "3.4.15",
    "@vue/eslint-config-airbnb": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "autoprefixer": "^10.4.17",
    "css-loader": "^6.9.1",
    "cypress": "^13.6.3",
    "dotenv": "^16.4.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^24.0.0",
    "prettier": "3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.70.0",
    "storybook": "^7.6.10",
    "storybook-dark-mode": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.2.1",
    "vue-style-loader": "^4.1.3",
    "vue-tsc": "^1.8.27"
  },
  "volta": {
    "node": "21.6.0"
  }
}
