{
  "name": "budget-tracker-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint -c ./.eslintrc.js --ignore-path ./.eslintignore",
    "dev": "vite",
    "prod": "vite --mode production",
    "docker-build": "docker build . -t letehaha/budget-tracker-fe",
    "docker-push": "docker push letehaha/budget-tracker-fe",
    "docker-compose": "npm run docker-build && npm run docker-push",
    "start": "npm run dev",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --passWithNoTests --config ./jest.config.js",
    "storybook": "storybook dev -p 6006",
    "test:e2e": "cypress run",
    "test:e2e:interact": "cypress open --browser chrome --e2e",
    "postinstall": "chmod +x ./post-install.sh && ./post-install.sh"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@vuelidate/core": "^2.0.2",
    "@vuelidate/validators": "^2.0.2",
    "core-js": "^3.31.0",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.1",
    "pinia": "^2.1.4",
    "register-service-worker": "^1.7.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.23",
    "@storybook/addon-docs": "^7.0.23",
    "@storybook/addon-essentials": "^7.0.23",
    "@storybook/addon-links": "^7.0.23",
    "@storybook/addons": "^7.0.23",
    "@storybook/cli": "^7.0.23",
    "@storybook/vue3": "^7.0.23",
    "@storybook/vue3-vite": "^7.0.23",
    "@types/jest": "^29.5.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/test-utils": "^2.4.0",
    "@vue/vue3-jest": "^29.2.4",
    "css-loader": "^6.8.1",
    "cypress": "^10.3.0",
    "dotenv": "^16.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-serializer-vue": "^3.1.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "markdown-loader-jest": "^0.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.55.0",
    "storybook": "^7.0.23",
    "storybook-dark-mode": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-svg-loader": "^4.0.0",
    "vue-style-loader": "^4.1.3",
    "vue-tsc": "^1.8.1"
  }
}
